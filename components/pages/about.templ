package pages

templ About(aboutMeTitle, aboutMeText string) {
	<div>
		<div class="max-w-4xl mx-auto p-6 gap-6 flex flex-col items-center">
			<img class="w-full md:w-auto md:max-w-full max-h-[60vh] object-cover" src="/static/upload/emma_about.jpg" alt="About Emma"/>
			<h2 class="text-2xl font-medium">{ aboutMeTitle }</h2>
			<div class="text-center whitespace-pre-line">
				{ aboutMeText }
			</div>
		</div>
	</div>
}
